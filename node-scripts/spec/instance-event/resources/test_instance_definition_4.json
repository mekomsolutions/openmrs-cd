{
  "name":"hsc-dev",
  "artifacts":[
    {
      "type":"maven",
      "value":{
        "groupId":"org.crudem",
        "artifactId":"bahmni-distro-hsc",
        "version":"1.1.0-SNAPSHOT",
        "packaging":"zip"
      }
    }
  ],
  "deployment":{
    "hostDir":"/var/volumes/bahmni",
    "type":"dockerCompose",
    "value":{
      "networks":[
        "network1"
      ],
      "commitId":"master",
      "gitUrl":"https://github.com/mekomsolutions/bahmni-docker.git"
    },
    "host":{
      "type":"ssh",
      "value":{
        "ip":"172.17.0.1",
        "user":"centos",
        "port":"22"
      }
    },
    "links":[],
    "timezone":"America/Port-au-Prince"
  },
  "description":"Dev server for agile development for HSC",
  "group":"hsc",
  "type":"dev",
  "version":"1.0.0",
  "properties":[
    {
      "filename":"erp.properties",
      "path":"/etc/properies",
      "service": "openmrs",
      "properties":{
        "erp.host":"${ODOO_HOST}",
        "erp.port":"${ODOO_PORT}",
        "erp.user":"${ODOO_USER}",
        "erp.password":"password",
        "erp.database":"odoo"
      }
    }
  ],
  "envVars":{
    "OPENMRS_MODULES_PATH":"/var/volumes/bahmni/hsc-dev/artifacts/openmrs_modules",
    "OPENMRS_CONFIG_PATH":"/var/volumes/bahmni/hsc-dev/artifacts/openmrs_config",
    "BAHMNI_CONFIG_PATH":"/var/volumes/bahmni/hsc-dev/artifacts/bahmni_config",
    "BAHMNI_APPS_PATH":"/var/volumes/bahmni/hsc-dev/artifacts/bahmni_emr/bahmniapps",
    "ODOO_EXTRA_ADDONS":"/var/volumes/bahmni/hsc-dev/artifacts/odoo_addons",
    "ODOO_CONFIG_PATH":"/var/volumes/bahmni/hsc-dev/artifacts/odoo_config"
  }
}